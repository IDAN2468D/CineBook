{
  "expo": {
    "name": "CineBook",
    "slug": "CineBook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cinebook",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.cinebook.app",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDgNF9eg-F14V2lrN7zTndhObyzhVXKAHU"
        }
      },
      "adaptiveIcon": {
        "backgroundColor": "#050B26",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      "expo-notifications",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow CineBook to use your location."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#050B26",
          "dark": {
            "backgroundColor": "#050B26"
          }
        }
      ]
    ],
    "experiments": {
      "reactCompiler": false,
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "053812dc-1198-4a04-8feb-211522639200"
      }
    }
  }
}